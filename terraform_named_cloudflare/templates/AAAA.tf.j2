{%- for resource in resources -%}
resource "cloudflare_record" "AAAA_{{ resource[0:-1] }}" {
  name       = "{{ resources[resource].name }}"
  ttl        = {{ resources[resource].ttl }}
  type       = "AAAA"
  value      = "{{ resources[resource].value }}"
  zone_id    = cloudflare_zone.{{ terrafromResource }}.id
}

{% endfor -%}