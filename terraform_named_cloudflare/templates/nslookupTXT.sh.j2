#!/bin/bash
 
# Declare a string array with type
declare -a StringArray=( "{{ parentZone }}"{{ space }}{%- for resource in resources -%} "{{ resources[resource].name  }}.{{ parentZone }}" {% endfor -%})
 
# Read the array values with space
for val in "${StringArray[@]}"; do
  nslookup -type=[TXT] $val {{ cloudflare_ns_record }} | grep "** server can't find" | awk '{print $5}' > errorTXT.txt 
done
